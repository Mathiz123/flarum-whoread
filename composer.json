{
    "name": "mathiz123/flarum-whoread",
    "description": "Make read status visible to selected groups",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "~1.0.4"
    },
    "authors": [
        {
            "name": "Mathiz123",
            "homepage": "xXx",
            "email": "xXx",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/Mathiz123/flarum-whoread",
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\WhoRead\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Who read",
            "category": "feature",
            "icon": {
                "name": "fas fa-book-reader",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": "https://discuss.flarum.org/d/23066"
        }
    }
}
